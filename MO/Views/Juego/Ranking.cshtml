<h2>🏆 Ranking de Jugadores</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Jugador</th>
            <th>Puntaje</th>
            <th>Fecha</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
            <tr>
                <td>@r.Username</td>
                <td>@r.Puntaje</td>
                <td>@r.Fecha.ToString("dd/MM/yyyy HH:mm")</td>
            </tr>
        }
    </tbody>
</table>
<a href="/MenuPrincipal" class="btn btn-primary">Regresar al Menú Principal</a>

